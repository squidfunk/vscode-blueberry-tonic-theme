	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
    },
    {
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
    },
		{
			"name": "Variable",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
    },
    {
			"name": "Variable constant",
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#FF5B97"
			}
		},
		{
			"name": "Other Variable",
			"scope": [
        "entity.other.attribute-name.terraform",
				"support.other.variable"
			],
			"settings": {
				"foreground": "#FF5B97"
			}
    },
    {
			"name": "Object keys",
			"scope": [
        "meta.object-literal.key",
				"variable.parameter.function-call"
			],
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"name": "Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
        "constant.character",
        "constant.escape",
        "keyword.operator.assignment",
        "keyword.operator.type",
				"keyword.other.unit",
        "keyword.other",
        "meta.function-call.arguments",
        "meta.jsx.children",
				"support.constant",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control.flow"
			],
			"settings": {
				"foreground": "#FF5B97"
			}
    },
    {
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FF5B97"
			}
    },
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"entity.tag.embedded",
				"meta.tag",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
			],
			"settings": {
				"foreground": "#FF5B97"
			}
    },
    {
			"name": "Tag",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#FF5B97"
			}
		},
    {
			"name": "Punctuation, Tag",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.arguments",
				"punctuation.definition.arguments.begin",
				"punctuation.definition.arguments.end"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
    },
    {
			"name": "Attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#FFBB7C"
			}
    },
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#97DEFF"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FDFF81"
			}
		},
		{
			"name": "C-related Block Level Variables",
			"scope": [
				"source.cpp meta.block variable.other"
			],
			"settings": {
				"foreground": "#FF5B97"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#84ffb7"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"variable.parameter.function.language.special"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "String, Symbol, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"punctuation.quote.double.terraform",
				"variable.assignment.terraform"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
    },
    {
			"name": "String Link",
			"scope": [
        "string.other.link"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"constant.language",
				"support.type"
			],
			"settings": {
				"foreground": "#84FFB7"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e280d2"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6B"
			}
    },
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
    },

    /* ------------------------------------------------------------------------
     * JavaScript
     * --------------------------------------------------------------------- */

    {
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#a2d5ff"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
    },

    /* ------------------------------------------------------------------------
     * CSS
     * --------------------------------------------------------------------- */

    {
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
